@model IEnumerable<MySportsBook.Model.Studio_IncomeDetail>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Layout/_AdminLayout.cshtml";
}

<main class="main">
    <!-- Breadcrumb-->
    <ol class="breadcrumb">
        <li class="breadcrumb-item">Home</li>
        <li class="breadcrumb-item">@Html.ActionLink("Incomes", "")</li>
        <li class="breadcrumb-item active">Index</li>
    </ol>
    <div class="container-fluid">
        <div class="animated fadeIn">
            <div class="card">
                <div class="card-header">
                    <i class="fa fa-align-justify"></i>
                    <strong>Income</strong>
                    <i class="fa fa-plus float-right">  @Html.ActionLink("Create New", "Create")</i>
                </div>
                <div class="card-body">
                    <table class="table table-striped table-bordered" style="width:100%">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.Studio_Event.CustomerName)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Studio_Event.OrderNumber)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ReceivedBy)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Description)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ReceivedDate)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Amount)
                                </th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Studio_Event.CustomerName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Studio_Event.OrderNumber)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ReceivedBy)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Description)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ReceivedDate)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Amount)
                                </td>
                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</main>


